#include <bits/stdc++.h>
using namespace std;


int main()
{
    long long int t;
    cin>>t;

    for(long long int i=0; i<t; i++){

       long long  int a,b,c;
        cin>>a>>b>>c;

        if(a>b){
            cout<<"First"<<endl;
        }
        else if(b>a)
            {
            cout<<"Second"<<endl;
        }
        else if((a+b+c) % 2 == 0){
            cout<<"Second"<<endl;
        }
        else{
            cout<<"First"<<endl;
        }
    }

}
