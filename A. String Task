#include <bits/stdc++.h>
using namespace std;

bool vowel(char c)
{
    return (c=='a' || c=='e'|| c=='i' || c=='o'||c=='u'||c=='y');
}

int main()
{
    string s;
    cin>>s;
    string ans;
    for(auto it : s){
        char c = tolower(it);
    
    if(vowel(c) == 0){
      ans += '.';
      ans += c;
    }
}
cout<<ans<<endl;
}
